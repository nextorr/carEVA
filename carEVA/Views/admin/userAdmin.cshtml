@model IEnumerable<carEVA.Models.evaUser>

@{
    ViewBag.Title = "userAdmin";
}

<h2>userAdmin</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.organization.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.organizationArea.areaCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.aspnetUserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.areaCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.publicKey)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.totalEnrollments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.completedCatalogCourses)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.completedRequiredCourses)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.organization.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.organizationArea.areaCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.aspnetUserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.areaCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.publicKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totalEnrollments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.completedCatalogCourses)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.completedRequiredCourses)
        </td>
        <td>
            @Html.ActionLink("Delete", "deleteEvaUser", new { id=item.ID })
        </td>
    </tr>
}

</table>
