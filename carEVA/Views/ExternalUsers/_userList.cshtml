@model IEnumerable<carEVA.Models.evaUser>

<h2>Usuarios en el Area</h2>

<p>
    @Html.ActionLink("Pre inscribir Usuario en grupo", "Create"
    ,new {externalAreaID = Request.QueryString["externalAreaID"] } )
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.organization.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.areaCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.totalEnrollments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.completedCatalogCourses)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.completedRequiredCourses)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.organization.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.areaCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totalEnrollments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.completedCatalogCourses)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.completedRequiredCourses)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
