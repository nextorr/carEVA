<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - EVA</title>

    @* include by default kendo in all the student views *@
    @Styles.Render("~/Content/kendo/2018.2.516/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/kendoui")
    @RenderSection("HeadStyles", required: false)
    @RenderSection("HeadScripts", required: false)
</head>
<body>
        @RenderBody()
        
    @RenderSection("FootScripts", required: false)
</body>
</html>